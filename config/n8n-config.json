{
  "database": {
    "type": "postgresdb",
    "postgresdb": {
      "host": "${DB_POSTGRESDB_HOST}",
      "port": "${DB_POSTGRESDB_PORT}",
      "database": "${DB_POSTGRESDB_DATABASE}",
      "username": "${DB_POSTGRESDB_USER}",
      "password": "${DB_POSTGRESDB_PASSWORD}",
      "ssl": {
        "enabled": false,
        "rejectUnauthorized": false
      },
      "poolSize": 2,
      "connectionTimeoutMillis": 20000,
      "idleTimeoutMillis": 180000,
      "maxUses": 5000
    }
  },
  "cache": {
    "backend": "memory",
    "memory": {
      "maxSize": 25,
      "ttl": 1800
    }
  },
  "queue": {
    "mode": "memory",
    "bull": {
      "redis": {
        "host": "disabled"
      }
    }
  },
  "executions": {
    "mode": "queue",
    "timeout": 600,
    "maxTimeout": 1200,
    "saveDataOnError": "all",
    "saveDataOnSuccess": "none",
    "saveDataManualExecutions": false,
    "pruneData": true,
    "pruneDataMaxAge": 72,
    "pruneDataTimeout": 1800
  },
  "workflows": {
    "callerPolicyDefaultOption": "any",
    "defaultName": "My workflow"
  },
  "credentials": {
    "overwrite": {
      "endpoint": ""
    }
  },
  "userManagement": {
    "disabled": false,
    "emails": {
      "mode": "smtp",
      "smtp": {
        "host": "${SMTP_HOST:-}",
        "port": "${SMTP_PORT:-587}",
        "secure": "${SMTP_SECURE:-false}",
        "auth": {
          "user": "${SMTP_USER:-}",
          "pass": "${SMTP_PASS:-}"
        }
      }
    },
    "jwtSecret": "${JWT_SECRET}",
    "sessionSecret": "${SESSION_SECRET}"
  },
  "nodes": {
    "exclude": [
      "n8n-nodes-base.executeCommand",
      "n8n-nodes-base.filesReadWrite"
    ],
    "errorTriggerType": "n8n-nodes-base.errorTrigger"
  },
  "security": {
    "excludeEndpoints": "none",
    "basicAuth": {
      "active": false
    },
    "jwtAuth": {
      "active": true,
      "jwtHeader": "authorization",
      "jwtHeaderValuePrefix": "Bearer",
      "jwksUri": "",
      "jwtIssuer": "",
      "jwtAudience": "",
      "userIdClaim": "sub",
      "userEmailClaim": "email"
    }
  },
  "endpoints": {
    "rest": "rest",
    "webhook": "webhook",
    "webhookWaiting": "webhook-waiting",
    "webhookTest": "webhook-test"
  },
  "externalFrontendHooks": "",
  "externalHookFiles": "",
  "logs": {
    "level": "${LOG_LEVEL:-warn}",
    "output": "console",
    "file": {
      "fileCountMax": 2,
      "fileSizeMax": 8,
      "location": "/tmp/n8n.log"
    }
  },
  "push": {
    "backend": "websocket"
  },
  "metrics": {
    "enable": false
  },
  "deployment": {
    "type": "default"
  },
  "versionNotifications": {
    "enabled": false
  },
  "templates": {
    "enabled": true,
    "host": "https://api.n8n.io/api/"
  },
  "binaryData": {
    "mode": "filesystem",
    "availableModes": "filesystem",
    "localStoragePath": "/tmp/n8n-binary-data",
    "binaryDataTTL": 30,
    "persistedBinaryDataTTL": 720
  },
  "publicApi": {
    "disabled": false,
    "path": "api",
    "swaggerUiDisabled": false
  },
  "license": {
    "serverUrl": "https://license.n8n.io/v1",
    "autoRenewal": {
      "enabled": false
    }
  }
}